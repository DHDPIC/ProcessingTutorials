// @DHDPIC

// ALL THE SIMPLE SHAPES

// SET SIZE
size(800,600);

// set background color
background(160,255,200);

// default fill is white & stroke is black
// rectangle
rect(50,50,200,150);

// set fill
fill(255,0,0);
// square
rect(400,50,200,200);

// set fill
fill(60,80,120);
// ellipse
ellipse(125,300,150,100);

// change stroke
stroke(255,60,180);
// circle
ellipse(300,350,100,100);

// remove stroke
noStroke();
// set fill
fill(255,240,90);
// triangle
triangle(200,400,400,500,100,550);

// add stroke back
stroke(10,120,40);
// set stroke weight
strokeWeight(3);
// set fill
fill(255,30,90);
// quadrangle
quad(500,300,700,400,750,500,550,550);
